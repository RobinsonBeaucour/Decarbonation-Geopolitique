	file = {ScienceDirect Full Text PDF:C\:\\Users\\Beaucour\\Zotero\\storage\\4QU3QTKE\\Lilliestam et Ellenbeck - 2011 - Energy security and renewable electricity trade—Wi.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Beaucour\\Zotero\\storage\\TA8R2X7N\\S0301421511002151.html:text/html},
