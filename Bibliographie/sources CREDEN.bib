@article{sovacool_major_2018,
	title = {Major hydropower states, sustainable development, and energy security: Insights from a preliminary cross-comparative assessment},
	volume = {142},
	issn = {0360-5442},
	url = {https://www.sciencedirect.com/science/article/pii/S0360544217315761},
	doi = {10.1016/j.energy.2017.09.085},
	shorttitle = {Major hydropower states, sustainable development, and energy security},
	abstract = {Hydropower remains, by a wide margin, the largest source of renewable electricity around the world, and hydropower dams supply a greater source of commercial energy than the world's fleet of nuclear power plants. Yet far less attention has been paid to the political, security, and governance elements underlying the continued use and growth of hydropower. Theorists from many disciplines—political geography, security studies, development studies, economics, public policy, and governance—have questioned both the proper role and ostensible benefits from the generation of electricity from large-scale hydroelectric dams, but have tended to approach the problem on a case-by-case basis. In this study, we provide the first systematic, cross-comparative assessment of five hypotheses about global hydropower states, using World Bank data from 1980 to 2010 and a research design which incorporates five reference classes of countries. Based on regression and comparative country analyses, we find tentative statistical evidence that hydropower countries do seem to perform worse on a series of security, economic development, and governance metrics.},
	pages = {1074--1082},
	journaltitle = {Energy},
	shortjournal = {Energy},
	author = {Sovacool, Benjamin K. and Walter, Götz},
	urldate = {2022-12-18},
	date = {2018-01-01},
	langid = {english},
	keywords = {01\_Risque dvp {ENR}, Energy security, Hydroelectric dams, Hydropower, Resource curse, Water wars},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\Beaucour\\Zotero\\storage\\PFHW764G\\Sovacool et Walter - 2018 - Major hydropower states, sustainable development, .pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Beaucour\\Zotero\\storage\\5I4YWUQE\\S0360544217315761.html:text/html},
