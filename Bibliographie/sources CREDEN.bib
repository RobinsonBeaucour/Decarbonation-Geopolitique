	langid = {english},
	keywords = {01\_Risque dvp {ENR}, Energy policy, Input-output analysis, Job creation, Life cycle analysis, Renewable energy, Sustainability},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\Beaucour\\Zotero\\storage\\VZRYB2J9\\Hondo et Moriizumi - 2017 - Employment creation potential of renewable power g.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Beaucour\\Zotero\\storage\\6NVSQFLT\\S1364032117306676.html:text/html},
