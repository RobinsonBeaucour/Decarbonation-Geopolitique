	file = {Full Text PDF:C\:\\Users\\Beaucour\\Zotero\\storage\\QTZRYIFF\\Paltsev - 2016 - The complicated geopolitics of renewable energy.pdf:application/pdf},
}

@article{escribano_frances_res_2013,
